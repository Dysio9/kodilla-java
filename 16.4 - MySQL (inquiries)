SELECT IL.NAME AS LIST_NAME, I.SUMMARY, I.DESCRIPTION
FROM ISSUESLISTS IL JOIN ISSUES I ON IL.ID = I.ISSUESLIST_ID;

SELECT U.FIRSTNAME, U.LASTNAME, I.SUMMARY, I.DESCRIPTION
FROM USERS U JOIN ISSUES I ON U.ID = I.USER_ID_ASSIGNEDTO;

SELECT U.FIRSTNAME, U.LASTNAME, COUNT(*) AS HOW_MANY_ISSUES FROM USERS U JOIN ISSUES I
ON U.ID = I.USER_ID_ASSIGNEDTO GROUP BY U.ID;